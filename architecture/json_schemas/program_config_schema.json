{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "buffer": {
            "properties": {
                "size": {
                    "type": "number",
                    "multipleOf": 1.0,
                    "minimum": 0
                }
            },
            "type": "object"
        },
        "include": {
            "items": {
                "enum": [
                    "actuators",
                    "sensors",
                    "services"
                ],
                "type": "string",
                "minLength": 1
            },
            "type": "array"
        },
        "influxdb": {
            "properties": {
                "bucket": {
                    "type": "string",
                    "minLength": 1
                },
                "host": {
                    "type": "string",
                    "minLength": 1,
                    "format": "uri"
                },
                "organization": {
                    "type": "string",
                    "minLength": 1
                },
                "password": {
                    "type": "string",
                    "minLength": 1
                },
                "username": {
                    "type": "string",
                    "minLength": 1
                }
            },
            "type": "object"
        },
        "tripleStore": {
            "properties": {
                "host": {
                    "type": "string",
                    "minLength": 1,
                    "format": "uri"
                },
                "password": {
                    "type": "string",
                    "minLength": 1
                },
                "username": {
                    "type": "string",
                    "minLength": 1
                }
            },
            "type": "object"
        }
    },
    "type": "object"
}

